<!doctype html>
<html>

<head>
  <title>Register for <%=shopData.shopName%>
  </title>
  <link rel="stylesheet" type="text/css" href="/main.css" />
</head>

<body>
  <h1>Register for <%=shopData.shopName%>
  </h1>
  <!-- Form to register a new user -->
  <form method="POST" action="registered">
    <p>First name: <input id="first" type="text" name="first" /></p>
    <p>Last name: <input id="last" type="text" name="last" /></p>
    <p>Email: <input id="email" type="email" name="email" /></p>
    <input type="submit" value="Register" />
  </form>
  <br>
  <a href="/">Back</a>
</body>

</html>