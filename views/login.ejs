<!doctype html>
<html>

<head>
  <title>Login for <%=shopData.shopName%>
  </title>
  <link rel="stylesheet" type="text/css" href="/main.css" />
</head>

<body>
  <h1>Login for <%=shopData.shopName%>
  </h1>
  <!-- Form to register a new user -->
  <form method="POST" action="loggedin">
    <p>User name: <input id="username" type="text" name="username" required /></p>
    <p>Password: <input id="password" type="password" name="password" required /></p>
    <input type="submit" value="Login" />
  </form>
  <br>
  <a href="/">Back</a>
</body>

</html>